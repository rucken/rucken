  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "engines": {
    "node": ">=12",
    "npm": ">=6.4.0"
  },
  "bugs": {
    "url": "https://github.com/rucken/rucken/issues"
  },
  "homepage": "https://github.com/rucken/rucken",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rucken/rucken.git"
  },
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "readme": "README.md",
  "scripts": {
    "nx": "nx",
    "start": "node ./dist/libs/rucken/src/main.js",
    "build": "npm run nx -- build rucken && ./scripts/add-header-to-rucken.js",
    "release": "standard-version",
    "publish": "npm publish ./dist/libs/rucken",
    "create-release": "npm run test && npm run build && npm run release && git push --follow-tags origin master",
    "test:e2e": "npm run nx -- test rucken --skip-nx-cache --json --outputFile=jest.result.json --testLocationInResults",
    "test:integrations-prepare": "sh ./integrations/scripts/prepare-application.sh",
    "test:integrations-run": "npm run test:e2e",
    "test": "npm run build && npm run test:integrations-prepare && npm run test:integrations-run",
    "lint": "npm run tsc:lint && nx workspace-lint && nx run-many --target=lint --all",
    "lint:fix": "npm run tsc:lint && nx workspace-lint --fix && nx run-many --target=lint --all --fix && nx format:write --all",
    "tsc:lint": "tsc --noEmit -p tsconfig.base.json"
  },
  "private": true,
  "devDependencies": {
    "@nrwl/cli": "13.4.1",
    "@nrwl/eslint-plugin-nx": "13.4.1",
    "@nrwl/jest": "13.4.1",
    "@nrwl/js": "13.4.1",
    "@nrwl/linter": "13.4.1",
    "@nrwl/node": "13.4.1",
    "@nrwl/tao": "13.4.1",
    "@nrwl/workspace": "13.4.1",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@types/recursive-readdir": "^2.2.0",
    "@types/replace-ext": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "~5.3.0",
    "@typescript-eslint/parser": "~5.3.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.2.3",
    "log4js": "^6.3.0",
    "nestjs-console": "^7.0.1",
    "prettier": "^2.3.1",
    "recursive-readdir": "^2.2.2",
    "replace-ext": "^2.0.0",
    "sort-paths": "^1.1.1",
    "standard-version": "^9.3.2",
    "ts-jest": "27.0.5",
    "typescript": "~4.3.5"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.4",
    "@rjaros/po2json": "^1.0.0-beta-4",
    "fast-deep-equal": "^3.1.3",
    "gettext-extractor": "^3.5.3",
    "i18next-conv": "10.2.0",
    "lodash.mergewith": "^4.6.2",
    "source-map-support": "^0.5.21",
    "tslib": "^2.0.0"
  }
}
